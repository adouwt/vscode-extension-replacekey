{
    "name": "vscode-plugin-replaceCtripSharkKey",
    "displayName": "vscode-plugin-replaceCtripSharkKey",
    "description": "VSCodeReplaceCtripSharkKey插件",
    "keywords": [
        "vscode",
        "plugin",
        "shark"
    ],
    "version": "1.0.0",
    "publisher": "erlinger",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "images/icon1.png",
    "activationEvents": [
        "*"
    ],
    "main": "./src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "VSCodeReplaceCtripSharkKey插件",
            "properties": {
                "vscodePluginDemo.yourName": {
                    "type": "string",
                    "default": "guest",
                    "description": "Todd Wang"
                }
            }
        },
        "commands": [
            {
                "command": "extension.replaceSharkKey",
                "title": "替换中文shark Key"
            }
        ],
        "keybindings": [
            {
                "command": "extension.replaceShark",
                "key": "ctrl+1",
                "mac": "cmd+1",
                "when": "editorTextFocus"
            }
        ],
        "menus": {
        },
        "snippets": [
            
        ],
        "viewsContainers": {
            
        },
        "views": {
           
        },
        "iconThemes": [
            {
                "id": "testIconTheme",
                "label": "测试图标主题",
                "path": "./theme/icon-theme.json"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "bugs": {
        "url": ""
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "homepage": ""
}
